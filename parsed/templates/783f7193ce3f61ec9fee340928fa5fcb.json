{"components/psdc-common/header-account-menu":"<li class=\"navUser-item navUser-item--account hide-mobile\">\n    <a class=\"navUser-action needsclick\" href=\"{{urls.account.index}}\">\n      <svg><use xlink:href=\"#icon-user\"></use></svg>\n      <!-- <span>{{lang 'common.account'}}</span> -->\n    </a>\n    <ul class=\"navUser-section-sub section-sub-right\">\n      {{#if customer}}\n        <li class=\"navUser-item\">\n          <a class=\"navUser-action\" href=\"{{urls.account.index}}\">\n              <svg><use xlink:href=\"#icon-user\"></use></svg><span>{{lang 'common.account'}}</span>\n          </a>\n        </li>\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action needsclick\" href=\"{{urls.auth.logout}}\">\n              <svg><use xlink:href=\"#icon-padlock\"></use></svg><span>{{lang 'common.logout'}}</span>\n            </a>\n        </li>\n      {{else}}\n      `\n        <li class=\"navUser-item\">\n          <a class=\"navUser-action needsclick\" href=\"{{urls.auth.login}}\">\n            <svg><use xlink:href=\"#icon-signout\"></use></svg><span>{{lang 'common.login'}}</span>\n          </a>\n        </li>\n        {{#if settings.account_creation_enabled}}\n        <li class=\"navUser-item\">\n          <a class=\"navUser-action needsclick\" href=\"{{urls.auth.create_account}}\">\n            <svg><use xlink:href=\"#icon-register\"></use></svg><span>{{lang 'common.sign_up'}}</span>\n          </a>\n        </li>\n        {{/if}}\n      {{/if}}\n      {{#if settings.gift_certificates_enabled}}\n          <li class=\"navUser-item\">\n              <a class=\"navUser-action needsclick\" href=\"{{urls.gift_certificate.purchase}}\">\n              <svg><use xlink:href=\"#icon-gift-certificate\"></use></svg><span>{{lang 'common.gift_cert'}}</span>\n              </a>\n          </li>\n      {{/if}}\n      {{#if settings.show_wishlist}}\n        <li class=\"navUser-item navUser-item-wishlist\">\n            <a class=\"navUser-action needsclick\" href=\"{{urls.account.wishlists.all}}\">\n              <svg><use xlink:href=\"#icon-wishlist\"></use></svg><span>{{lang 'account.nav.wishlist'}}</span>\n            </a>\n        </li>\n      {{/if}}\n\n      {{#if customer.store_credit.value '>' 0}}\n          <li class=\"navUser-item navUser-item-storeCredit\">\n              <a class=\"navUser-action navUser-action--storeCredit needsclick\" href=\"{{urls.cart}}\">\n                 <span>{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\n              </a>\n          </li>\n      {{/if}}\n\n    </ul>\n</li>\n<li class=\"navUser-item hide-desktop\">\n    <a class=\"navUser-action\" onClick=\"document.getElementById('mobile-search').classList.toggle('hide-mobile');\">\n      <svg><use xlink:href=\"#icon-search\"></use></svg>\n    </a> \n</li>\n","components/psdc-common/navigation":"<nav class=\"navUser\">\r\n    <ul class=\"navUser-section navUser-section--alt account-section sf-menu\">\r\n      {{> components/psdc-common/header-account-menu}}\r\n\r\n    {{#if theme_settings.EnableProductCompare}}\r\n      <li class=\"navUser-item navUser-item-compare\">\r\n          <a class=\"navUser-action navUser-item--compare needsclick\" title=\"Compare\" href=\"{{urls.compare}}\" data-compare-nav>\r\n            <svg><use xlink:href=\"#icon-compare\"></use></svg>\r\n            <span class=\"countPill countPill--positive\"></span>\r\n          </a>\r\n      </li>\r\n    {{/if}}\r\n\r\n      <li class=\"navUser-item navUser-search\">\r\n          <a class=\"navUser-action navUser-action--quickSearch\" href=\"javascript:void(0);\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\"><svg class=\"search-icon\"><use xlink:href=\"#icon-search\"></use></svg> <svg class=\"search-close\"><use xlink:href=\"#icon-close\"></use></svg></a>\r\n      </li>\r\n\r\n\r\n      <li class=\"navUser-item navUser-item--cart\">\r\n          <a  class=\"navUser-action\"\r\n              data-cart-preview\r\n              data-dropdown=\"cart-preview-dropdown\"\r\n              data-options=\"align:right\"\r\n              href=\"{{urls.cart}}\">\r\n              <span class=\"navUser-item-cartLabel\">\r\n                <svg><use xlink:href=\"#icon-cart\"></use></svg>\r\n              </span> <span class=\"countPill cart-quantity\"></span>\r\n          </a>\r\n          <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n      </li>\r\n    </ul>\r\n\r\n</nav>\r\n\r\n<div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n    {{> components/psdc-common/quick-search headerSearch='headerSearch'}}\r\n</div>\r\n\r\n<div id=\"headerSidebar\" class=\"header-side-bar\">\r\n<div class=\"header-side-bar-inn\">\r\n  <a href=\"{{urls.home}}\"><img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\"></a>\r\n  <div class=\"close\">\r\n    <a class=\"close-icon\" href=\"javascript:void(0);\"><svg><use xlink:href=\"#icon-close\"></use></svg></a>\r\n  </div>\r\n\r\n  <div class=\"category headerSidebar-block\">\r\n    <ul class=\"navList navList-treeview\">\r\n        <li class=\"navList-item\"><a class=\"navPages-action\" href=\"/categories\" class=\"needsclick\">BLUE LIGHT SCREEN PROTECTION</a></li>\r\n        {{#each categories}}\r\n            <li class=\"navPages-item\">\r\n              {{> components/psdc-common/navigation-list}}\r\n            </li>\r\n      {{/each}}\r\n    </ul>\r\n  </div>\r\n  <div class=\"webpages headerSidebar-block\">\r\n    <ul class=\"navList navList-treeview\">\r\n    {{#each pages}}\r\n        <li class=\"navList-item\"><a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">{{ name }}</a></li>\r\n        {{#each children}}\r\n            <li class=\"navList-item\"><a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">{{ name }}</a></li>\r\n        {{/each}}\r\n\r\n    {{/each}}\r\n    </ul>\r\n  </div>\r\n  <div class=\"account headerSidebar-block\">\r\n    <ul class=\"navList navList-treeview treeview\">\r\n      {{#if customer}}\r\n        <li class=\"navList-item\">\r\n          <a class=\"navPages-action\" href=\"{{urls.account.index}}\">\r\n              {{lang 'common.account'}}\r\n          </a>\r\n        </li>\r\n        <li class=\"navList-item\">\r\n            <a class=\"navPages-action\" href=\"{{urls.auth.logout}}\">\r\n              {{lang 'common.logout'}}\r\n            </a>\r\n        </li>\r\n      {{else}}\r\n        <li class=\"navList-item\">\r\n          <a class=\"navPages-action\" href=\"{{urls.auth.login}}\">\r\n            {{lang 'common.login'}}\r\n          </a>\r\n        </li>\r\n      {{/if}}\r\n    </ul>\r\n  </div>\r\n  {{#if currency_selector.currencies.length '>' 1}}\r\n    <div class=\"brand headerSidebar-block mobile-currancy\">\r\n      <h5 class=\"sidebarBlock-heading\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <span class=\"flag\">{{{lang 'common.currency-flag' flag=currency_selector.active_currency_flag}}}</span><span class=\"code\">{{lang 'common.currency-code' code=currency_selector.active_currency_code}}</span></h5>\r\n\r\n    </div>\r\n  {{/if}}\r\n</div>\r\n\r\n</div>\r\n","components/psdc-common/navigation-list":"<!-- SECOND LEVEL STARTS -->\n{{#if children}}\n\n<a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n    {{name}}\n    <svg class=\"chevron-down\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n</a>\n<ul class=\"navlist-sub nav-second-level\">\n    {{#each children}}\n    <li>\n        <!-- THIRD LEVEL STARTS -->\n        {{#if children}}\n        <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n          <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></svg></span>\n            {{name}} <svg class=\"chevron-down\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n        </a>\n        <ul class=\"navlist-sub nav-third-level\">\n            {{#each children}}\n            <li>\n              <!-- FOURTH LEVEL STARTS -->\n              {{#if children}}\n              <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n                <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></svg></span>\n                  {{name}} <svg class=\"chevron-down\"><use xlink:href=\"#icon-chevron-down\"></use></svg>\n              </a>\n              <ul class=\"navlist-sub nav-third-level\">\n                  {{#each children}}\n                  <li>\n                      <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">{{name}} </a>\n                  </li>\n                  {{/each}}\n              </ul>\n              {{else}}\n                <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">{{name}}</a>\n              {{/if}}\n              <!-- FOURTH LEVEL STARTS -->\n\n            </li>\n            {{/each}}\n        </ul>\n        {{else}}\n        <a class=\"navPages-action\" href=\"{{url}}\" class=\"needsclick\">\n          <span class=\"nav-arrow\"><svg class=\"\"><use xlink:href=\"#icon-m-right\"></use></svg></span>{{name}}</a>\n        {{/if}}\n        <!-- THIRD LEVEL STARTS -->\n    </li>\n    {{/each}}\n</ul>\n{{else}}\n<a class=\"navPages-action\" href=\"{{url}}\">\n  {{name}}\n</a>\n{{/if}}\n<!-- SECOND LEVEL ENDS -->\n","components/psdc-common/quick-search":"<form class=\"form\" action=\"/search-results\" id=\"searchform\" name=\"searchform\">\n    <fieldset class=\"form-fieldset\" id=\"searchfields\">\n        <div class=\"form-field\">\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input\" name=\"ss360Query\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n        </div>\n    </fieldset>\n</form>\n"}
